          .rotatingText {
  font-family: "Georgia", serif;
  font-style: italic;
  font-size: 18px;
  text-align: center;
  position: relative;
}

.rotatingText-adjective {
    font-family: "Open Sans", sans-serif;
    font-size: 22px;
    font-style: normal;
    font-weight: 700;
    left: 0;
    opacity: 0;
    position: fixed;
    text-align: left;
    right: 0;
    text-transform: uppercase;
    top: 0;
    padding-left: 100px;
    padding-top:1rem;
    animation: rotate 9s infinite;
}

.rotatingText-adjective:nth-of-type(1) {
  animation-delay: 0s;
}

.rotatingText-adjective:nth-of-type(2) {
  animation-delay: 3s;
}

.rotatingText-adjective:nth-of-type(3) {
  animation-delay: 6s;
}


@keyframes rotate {
  0% { opacity: 0; transform: translateY(15px); }
  10%, 30% { opacity: 1; transform: translateY(0); }
  40%, 100% { opacity: 0; transform: translateY(-15px); }
}
  
