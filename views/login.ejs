         style="margin: 15px auto; width: 60%; max-width: 500px; padding: 15px; 
                border-radius: 8px; font-size: 16px; font-weight: 500; 
                box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.2); background-color: #e3f2fd; color: #0d47a1;">
        <strong>ℹ️ Info:</strong> <%= alertMessage %>
        <button type="button" class="close" data-dismiss="alert" aria-label="Close" style="font-size: 18px; color: #0d47a1;">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
<% } %>

 
